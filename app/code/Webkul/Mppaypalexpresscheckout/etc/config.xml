<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <mppaypalexpresscheckout>
                <model>Webkul\Mppaypalexpresscheckout\Model\PaymentMethod</model>
                <paypal_app_id backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <api_username backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <api_password backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <api_signature backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <!-- <client_id backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <client_secret backend_model="Magento\Config\Model\Config\Backend\Encrypted" /> -->
                <title>Webkul Marketplace PayPal Express Checkout Payment Method</title>
                <payment_action>authorize</payment_action>
                <active>0</active>
                <sandbox>1</sandbox>
                <is_invoice_manage>1</is_invoice_manage>
                <allowspecific>0</allowspecific>
                <verify_seller>0</verify_seller>
                <details_approval>1</details_approval>
            </mppaypalexpresscheckout>
        </payment>
        <marketplace>
            <email>
                <expresscheckout_status_template>marketplace_email_expresscheckout_status_template</expresscheckout_status_template>
            </email>
        </marketplace>
    </default>
</config>
